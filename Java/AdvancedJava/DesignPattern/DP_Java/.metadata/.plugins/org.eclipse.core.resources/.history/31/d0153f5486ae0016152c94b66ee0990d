package com.crossover.journals.Services;

import org.springframework.stereotype.Service;

import com.crossover.CrossOverJournals.DAO.UserDAO;

/**
 * 
 * Service: UserServiceImpl
 * @Author: Arun Dhwaj
 * @Date: 19th Nov'16
 * 
 */

@Service("UserService")
public class UserServiceImpl implements UserService 
{
	
	private UserDAO userDAO;
	
	public void setUserDAO(UserDAO userDAO) 
    {
		System.out.println("1.1 UserServiceImpl: setUserDAO: userDAO :" + userDAO);
        this.userDAO = userDAO;
    }
	
}
